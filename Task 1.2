n = 5

arr = [[None] * n for i in range(n)]

current = 1

x,y = 0, 0
while arr[x][y] is None:
  
  arr[x][y] = current
  current += 1

  if y + 1 < n and arr[x][y+1] is None:
    y += 1
  elif x + 1 < n and arr[x+1][y] is None:
    x += 1
  elif y - 1 > -1 and arr[x][y-1] is None:
    y -= 1
  elif x - 1 > -1 and arr[x-1][y] is None:
    x -= 1

print(arr)
